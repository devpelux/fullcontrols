<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
	<IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
    <Authors>Salvatore Peluso</Authors>
    <Description>Demo project of FullControls library.</Description>
    <Copyright>Copyright 2020 Salvatore Peluso</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/devpelux/fullcontrols</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/devpelux/fullcontrols</RepositoryUrl>
	<PackageTags>Controls, Custom, Full, TextBox, PasswordBox, ComboBox, Button, ToggleButton, RepeatButton, ScrollViewer, ScrollBar, MenuItem, Window</PackageTags>
	<PackageReleaseNotes>Updated for the new release of FullControls.</PackageReleaseNotes>
    <ApplicationIcon>Icons\FullControls.ico</ApplicationIcon>
    <StartupObject>FullControlsDemo.App</StartupObject>
    <Version>1.1.2</Version>
    <AssemblyVersion>1.1.2.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Icons\FullControls.ico" />
    <None Remove="Icons\ic_label.png" />
    <None Remove="Icons\ic_lock.png" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\FullControls\FullControls.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Icons\FullControls.ico" />
    <Resource Include="Icons\ic_label.png" />
    <Resource Include="Icons\ic_lock.png" />
  </ItemGroup>

</Project>
